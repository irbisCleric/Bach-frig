// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.

{
  "extends": "airbnb",
   "rules": {
     "linebreak-style": ["error", "unix"],
     "quotes": ["error", "double"],
     "semi": "error",
     "indent": ["error", 4],
     "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
     "no-var": "error",
     "no-console": "warn",
     "no-debugger": "warn"
   },
   "env": {
     "browser": true,
     "node": true
   }
}
